{
  "name": "wallet-login",
  "version": "0.1.0",
  "private": true,
  "main": "index.html",
  "homepage": "https://olderfall.github.io/wallet-login",
  "scripts": {
    "start": "parcel index.html --open",
    "build": "shx rm -rf dist/* && parcel build ./index.html --public-url /wallet-login --target node --no-source-maps --bundle-node-modules --out-dir ./dist --out-file index.html",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  }, 
  "dependencies": {
    "ethers": "5.5.1",
    "parcel-bundler": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "typescript": "4.4.4",
    "shx": "^0.3.2"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "engines": {
    "parcel": "2.x",
    "node" : ">=16.0.0 <17.0.0"
  },
  "keywords": [
    "javascript",
    "starter"
  ]
}
